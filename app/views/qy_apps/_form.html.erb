<%= form_for @qy_app, :html => { :class => 'form-horizontal' } do |f| %>
    <% if @qy_app.errors.any? %>
        <div id="error_explanation" class="row">
          <h2><%= pluralize(@qy_app.errors.count, "error") %> prohibited this qy_app from being saved:</h2>
          <ul>
            <% @qy_app.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <div class="field form-group">
      <%= f.label :name, :class => "col-sm-2 control-label" %>
      <div class="col-sm-10">
       <%= f.text_field :name, :class => 'form-control' %>
      </div>
    </div>
    <div class="field form-group">
      <%= f.label :qy_token, :class => "col-sm-2 control-label" %>
      <div class="col-sm-10">
      <%= f.text_field :qy_token, :class => 'form-control' %>
      </div>
    </div>
    <div class="field form-group">
      <%= f.label :corp_id, :class => "col-sm-2 control-label" %>
      <div class="col-sm-10">
      <%= f.text_field :corp_id, :class => 'form-control' %>
      </div>
    </div>
    <div class="field form-group">
      <%= f.label :encoding_aes_key, :class => "col-sm-2 control-label" %>
      <div class="col-sm-10">
        <%= f.text_field :encoding_aes_key, :class => 'form-control' %>
      </div>
    </div>
    <div class="actions form-group row">
      <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit 'Submit', :class => 'form-control' %>
        </div>
    </div>
<% end %>